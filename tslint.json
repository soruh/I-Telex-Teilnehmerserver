{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly":false,

        "prefer-const": false,
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "only-arrow-functions":false,
        "no-trailing-whitespace":false,
        "typedef-whitespace":false,
        "no-consecutive-blank-lines":false,
        "ordered-imports":false,
        "radix":false,
        "quotemark":false,
        "whitespace":false,
        "one-line":false,
        "interface-name":false,
        "variable-name":false,
        "indent": [true, "tabs", 4],
        "class-name":false,
        "no-empty-interface":false,
        "no-bitwise":false,
        "forin":false,
        "no-string-literal":false,
        "no-shadowed-variable":false,
        "max-line-length":false,

        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ]

    },
    "rulesDirectory": []
}