{
  "name": "i-telex-teilnehmerserver",
  "version": "5.0.0",
  "description": "A subscribtion server for the I-Telex system",
  "repository": {
    "type": "git",
    "url": "https://github.com/soruh/I-Telex-Teilnehmerserver/"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 delete ecosystem.config.js",
    "restart": "pm2 reload ecosystem.config.js",
    "logs": "pm2 logs",
    "list": "pm2 list",
    "docker_start": "docker start teilnehmerserver",
    "docker_stop": "docker stop teilnehmerserver",
    "docker_restart": "docker restart teilnehmerserver",
    "docker_logs": "tools/containerExec.sh pm2 logs",
    "docker_list": "tools/containerExec.sh pm2 list",
    "docker_init": "tools/startmysql.sh&&tools/compileDocker.sh&&tools/runDocker.sh -d",
    "docker_build": "tools/compileDocker.sh",
    "docker_run": "tools/runDocker.sh -d",
    "docker_run_once": "tools/runDocker.sh",
    "docker_exec": "tools/containerExec.sh",
    "docker_top": "docker top teilnehmerserver",
    "docker_stats": "docker stats teilnehmerserver",
    "docker_dump_db": "npm run docker_dump_db_stdout > dump.sql",
    "docker_dump_db_stdout": "docker exec mysql mysqldump --all-databases -utelefonbuch -pxNig4esW",
    "docker_manage_servers": "docker exec teilnehmerserver node tools/manage_servers.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "express": "^4.16.3",
    "ip": "^1.1.5",
    "morgan": "^1.9.1",
    "nodemailer": "^4.4.1",
    "pug": "^2.0.0-rc.4",
    "serve-favicon": "~2.4.2",
    "sqlstring": "^2.3.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.5",
    "@types/sqlite3": "^3.1.3",
    "@types/sqlstring": "^2.2.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/heapdump": "^0.3.1",
    "@types/intl": "^1.2.0",
    "@types/ip": "0.0.31",
    "@types/jquery.validation": "^1.16.4",
    "@types/morgan": "^1.7.35",
    "@types/nodemailer": "^4.6.2",
    "@types/serve-favicon": "^2.2.30",
    "@types/underscore": "^1.8.8"
  },
  "author": "soruh",
  "license": "ISC"
}
